
@{
    ViewBag.Title = "Client Report";
}

@model IEnumerable<SSSLLP.Models.ClientReportingTbl>
<hr />
<section class="bxs-report my-5">
    <div class="container">
        <div class="col-lg-12">
            <div class="page-wrapper">
                <div class="page-content">

                    <h6 class="mb-0 text-uppercase">
                        Client Report DataTable &nbsp;
                        <a href="~/Account/Export" class="btn btn-success px-3" style="background-color: #157347;color:white;">Download</a>
                    </h6>
                    <hr>
                    <div class="card">
                        <div class="card-body">
                            <div class="table-responsive">
                                <table id="example" class="table table-striped table-bordered" style="width:100%">
                                    <thead>
                                        <tr>
                                            <th>Id</th>
                                            <th>LoomNo</th>
                                            <th>OwnParty</th>
                                            <th>QualityNo</th>
                                            <th>ReedxPickxRs</th>
                                            <th>BeamNo</th>
                                            <th>BeamPipeNo</th>
                                            <th>DateOfGaitingKnotting</th>
                                            <th>Nos</th>
                                            <th>AvgOfRollingMtr</th>
                                            <th>AvgOfPick</th>
                                            <th>TotalFabricReced</th>
                                            <th>BeamBalanceMtr</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @foreach (var item in Model)
                                        {
                                            <tr>
                                                <td>@item.Id</td>
                                                <td>@item.LoomNo</td>
                                                <td>@item.OwnParty</td>
                                                <td>@item.QualityNo</td>
                                                <td>@item.ReedxPickxRs</td>
                                                <td>@item.BeamNo</td>
                                                <td>@item.BeamPipeNo</td>
                                                <td>@item.DateOfGaitingKnotting</td>
                                                <td>@item.BillFallDate</td>
                                                <td>@item.Nos</td>
                                                <td>@item.AvgOfRollingMtr</td>
                                                <td>@item.AvgOfPick</td>
                                                <td>@item.TotalFabricReced</td>
                                                <td>@item.BeamBalanceMtr</td>
                                            </tr>
                                        }
                                    </tbody>

                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<hr />


