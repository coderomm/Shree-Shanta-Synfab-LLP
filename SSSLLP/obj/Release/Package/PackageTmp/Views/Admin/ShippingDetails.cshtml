@model SSSLLP.Models.ShippingDetail

@{
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<div class="page-wrapper">
    <div class="page-content">
        <!--breadcrumb-->
        <div class="page-breadcrumb d-none d-sm-flex align-items-center mb-3">
            <div class="breadcrumb-title pe-3">Forms</div>
            <div class="ps-3">
                <nav aria-label="breadcrumb">
                    <ol class="breadcrumb mb-0 p-0">
                        <li class="breadcrumb-item">
                            <a href="javascript:;"><i class="bx bx-home-alt"></i></a>
                        </li>
                        <li class="breadcrumb-item active" aria-current="page">Form Layouts</li>
                    </ol>
                </nav>
            </div>

        </div>
        <!--end breadcrumb-->

        <h6 class="mb-0 text-uppercase">Product Sample Order Shipping Details</h6>
        <hr>
        <div class="card">
            <div class="card-body">
                <div class="card-title d-flex align-items-center">
                    <h5 class="mb-0 text-danger">Product Sample Order Shipping Details</h5>
                </div>
                <hr>
                <div class="table-responsive">
                    <table id="example" class="table table-striped table-bordered" style="width:100%">
                        <thead>
                            <tr>
                                <th>Shipping ID : @Model.ShippingID</th>
                            </tr>
                            <tr>
                                <th>Order ID : @Model.OrderID</th>
                            </tr>
                            <tr>
                                <th>First Name : @Model.FirstName</th>
                            </tr>
                            <tr>
                                <th>Last Name : @Model.LastName</th>
                            </tr>
                            <tr>
                                <th>Email : @Model.Email</th>
                            </tr>
                            <tr>
                                <th>Mobile : @Model.Mobile</th>
                            </tr>
                            <tr>
                                <th>Address : @Model.Address</th>
                            </tr>
                            <tr>
                                <th>State : @Model.State</th>
                            </tr>
                            <tr>
                                <th>City : @Model.City</th>
                            </tr>
                            <tr>
                                <th>Post Code : @Model.PostCode</th>
                            </tr>
                            <tr>
                                <th>Order Note : @Model.OrderNote</th>
                            </tr>
                        </thead>
                    </table>
                    <br />
                    <a href="@Url.Action("ProductSampleOrderEnquires", "Admin")" class="btn btn-primary">Back To Product Sample Order Enquires</a>

                </div>
            </div>
        </div>
    </div>
</div>