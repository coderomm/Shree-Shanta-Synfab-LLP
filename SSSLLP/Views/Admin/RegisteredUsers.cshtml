@model IEnumerable<SSSLLP.Models.Customers_Tbl>
@{
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<div class="page-wrapper">
    <div class="page-content">
        <!--breadcrumb-->
        <div class="page-breadcrumb d-none d-sm-flex align-items-center mb-3">
            <div class="breadcrumb-title pe-3">Forms</div>
            <div class="ps-3">
                <nav aria-label="breadcrumb">
                    <ol class="breadcrumb mb-0 p-0">
                        <li class="breadcrumb-item">
                            <a href="javascript:;"><i class="bx bx-home-alt"></i></a>
                        </li>
                        <li class="breadcrumb-item active" aria-current="page">Form Layouts</li>
                    </ol>
                </nav>
            </div>

        </div>
        <!--end breadcrumb-->

        <h6 class="mb-0 text-uppercase">Registered Customers / Users</h6>
        <hr>
        <div class="card">
            <div class="card-body">
                <div class="card-title d-flex align-items-center">
                    <h5 class="mb-0 text-danger">Registered Customers / Users</h5>
                </div>
                <hr>
                <div class="table-responsive">
                    <table id="example" class="table table-striped table-bordered" style="width:100%">
                        <thead>
                            <tr>
                                <th width="5%">Id</th>
                                <th width="10%">First<br />Name</th>
                                <th width="10%">Last<br />Name</th>
                                <th width="10%">UserName</th>
                                <th width="10%">State</th>
                                <th width="10%">City</th>
                                <th width="10%">Email</th>
                                <th width="10%">Mobile</th>
                                <th width="15%">Address</th>
                                <th width="10%">Allow<br />Access</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td>@item.Id</td>
                                    <td>@item.First_Name</td>
                                    <td>@item.Last_Name</td>
                                    <td>@item.UserName</td>
                                    <td>@item.State</td>
                                    <td>@item.City</td>
                                    <td>@item.Email</td>
                                    <td>@item.Mobile</td>
                                    <td>@item.Address</td>
                                    <td>@item.rts</td>
                                </tr>
                            }
                        </tbody>

                    </table>
                </div>
            </div>
        </div>
    </div>
</div>